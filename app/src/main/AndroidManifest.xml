<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demosqllite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CourseRowActivity"></activity>
        <activity android:name=".activity.ListCourseActivity" />
        <activity android:name=".activity.student.StudentActivity" />
        <activity android:name=".activity.student.StudentRowActivity" />
        <activity android:name=".activity.student.ListStudentActivity" />
        <activity android:name=".activity.student.CreateStudentActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>